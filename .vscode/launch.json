{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [

    {
      "name": "Generate",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1",
      "program": "${workspaceRoot}/ipsws.go",
      "env": {},
      "args": ["generate"],
      "showLog": false
    },
    {
      "name": "Device",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1",
      "program": "${workspaceRoot}/cmd/ipsw/main.go",
      "env": {},
      "args": ["-V", "device", "${workspaceRoot}/Firmware/all_flash/DeviceTree.d321ap.im4p"],
      "showLog": false
    },
    {
      "name": "Download - Latest",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1",
      "program": "${workspaceRoot}/cmd/ipsw/main.go",
      "env": {},
      "args": ["-V", "download", "latest"],
      // "args": ["-V", "itunes", "--build", "16F156"],
      "showLog": false
    },
    {
      "name": "Download - Kernel",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1",
      "program": "${workspaceRoot}/cmd/ipsw/main.go",
      "env": {},
      "args": ["-V", "download", "kernel", "--version", "12.4"],
      // "args": ["-V", "itunes", "--build", "16F156"],
      "showLog": false
    },
    {
      "name": "Extract",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1",
      "program": "${workspaceRoot}/ipsws.go",
      "env": {},
      "args": ["-V", "extract", "${workspaceRoot}/iPhone11,2_12.3_16F156_Restore.ipsw"],
      "showLog": false
    },
    {
      "name": "Extract - dyld",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1",
      "program": "${workspaceRoot}/ipsws.go",
      "env": {},
      "args": ["-V", "extract", "--dyld", "${workspaceRoot}/iPhone11,2_12.3_16F156_Restore.ipsw"],
      "showLog": false
    },
    {
      "name": "Extract - WebKit Version",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1",
      "program": "${workspaceRoot}/ipsws.go",
      "env": {},
      "args": ["-V", "webkit", "${workspaceRoot}/dyld_shared_cache"],
      "showLog": false
    },
    {
      "name": "Decompress",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1",
      "program": "${workspaceRoot}/ipsws.go",
      "env": {},
      "args": ["-V", "decompress", "${workspaceRoot}/iPhone11,2_12.3_16F156_Restore/kernelcache.release.iphone11"],
      "showLog": false
    },
    {
      "name": "Diff",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1",
      "program": "${workspaceRoot}/ipsws.go",
      "env": {},
      "args": [
        "-V",
        "diff",
        "test-caches/kernelcache.release.iphone11.decompressed"
      ],
      "showLog": false
    },
    {
      "name": "Extract DYLD",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1",
      "program": "${workspaceRoot}/ipsws.go",
      "env": {},
      "args": [
        "-V",
        "extract",
        "--dyld",
        "iPhone11,2_12.0_16A366_Restore.ipsw"
      ],
      "showLog": false
    },
    {
      "name": "Query",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1",
      "program": "${workspaceRoot}/ipsws.go",
      "env": {},
      "args": ["-V", "-b", "12.0"],
      "showLog": false
    }
  ]
}
